<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#a7621c" />
            <gradient  android:angle="-90"  android:startColor="#3b2213" android:endColor="#c97724"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#976e25" />
            <solid  android:color="#be711e"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#e28016" />
            <gradient  android:angle="-90"  android:startColor="#e7be55" android:endColor="#bf7310" />
        </shape>
    </item>
</selector>